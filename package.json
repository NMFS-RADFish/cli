{
  "name": "cli",
  "version": "0.1.1",
  "description": "A cli tool for bootstrapping a radfish app",
  "bin": {
    "create-radfish-app": "./index.js"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js",
    "format": "prettier --write src --config .prettierrc",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ]
  },
  "main": "index.js",
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@inquirer/select": "^1.3.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0"
  }
}
